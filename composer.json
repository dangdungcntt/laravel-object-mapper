{
    "name": "nddcoder/object-mapper",
    "description": "Object mapper for PHP",
    "keywords": [
        "nddcoder",
        "object-mapper"
    ],
    "homepage": "https://github.com/dangdungcntt/object-mapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Dung Nguyen Dang",
            "email": "dangdungcntt@gmail.com",
            "homepage": "https://nddcoder.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Nddcoder\\ObjectMapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nddcoder\\ObjectMapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "psalm",
        "test": "phpunit --colors=always",
        "test-coverage": "XDEBUG_MODE=coverage phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
